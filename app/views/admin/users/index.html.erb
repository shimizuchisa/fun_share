<h1>Admin::Users#index</h1>
<p>Find me in app/views/admin/users/index.html.erb</p>


<%= render 'layouts/admin_header' %>
<div class="container">
  <h2>会員一覧</h2>
  <div class="row">
    <div class="col-md-10">
      <table class="table">
        <thead>
          <tr>
            <th>会員ID</th>
            <th>名前</th>
            <th>ステータス</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td><%= @user.id %></td>
            <td><%= @user.last_name %><%= @user.first_name %></td>
            <td><%= @user.is_deleted %></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>

